{
  "name": "tyc-backend",
  "version": "0.0.1",
  "description": "Node.js backend for thank you cookies",
  "main": "dist/app.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:ci": "jest --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch --passWithNoTests",
    "build:watch": "tsc --watch && nodemon",
    "build": "tsc",
    "build:prod": "tsc --build tsconfig.prod.json",
    "coverage": "jest --coverage",
    "lint": "tslint --fix **/*.ts",
    "prettify": "prettier --write",
    "dev": "concurrently --kill-others \"npm run build:watch\" nodemon",
    "start": "node ./app.js",
    "db-migrate:create": "db-migrate create",
    "db-migrate:up": "db-migrate up",
    "db-migrate:down": "db-migrate down"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettify",
      "npm run lint",
      "git add"
    ],
    "*.{scss,json,md,js,jsx,mdx,yml}": [
      "npm run prettify",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThankUCookies/tyc-backend.git"
  },
  "keywords": [
    "node",
    "typescript"
  ],
  "author": "ameerthehacker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ThankUCookies/tyc-backend/issues"
  },
  "jest-junit": {
    "outputDirectory": "./__test__",
    "outputName": "./test-results.xml"
  },
  "homepage": "https://github.com/ThankUCookies/tyc-backend#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/express-validator": "^3.0.0",
    "@types/jest": "^23.3.11",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/node": "^10.12.18",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/sequelize": "^4.27.33",
    "concurrently": "^4.1.0",
    "db-migrate": "^0.11.5",
    "db-migrate-mysql": "^1.1.10",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-junit": "^6.0.1",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.4.0",
    "mysql2": "^1.6.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^5.15.1",
    "sequelize-typescript": "^0.6.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  }
}
